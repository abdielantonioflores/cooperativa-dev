                                      <input id="departmentsAddressCity" class="span6" type="text" value=""
                                        name="departmentsAddressCity" />
                                      <label for="departmentsDV" class="control-label-dv">DV <span
                                          class="required">!</span></label>
                                      <input id="departmentsDV" class="span2" type="text" value=""
                                        name="departmentsAddressCity" />
