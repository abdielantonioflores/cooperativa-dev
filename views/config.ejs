                                    <div class="controls controls-row">
                                      <input id="departmentsJob" class="span6 margin-right5" type="text" value=""
                                        name="departmentsJob" />
                                    </div>
                                  </li>
                                  <!-- // form item -->
                                  <li class="control-group">
                                    <label for="departmentsAddressStreet" class="control-label">Direccion <span
                                        class="required">!</span></label>
                                    <div class="controls controls-row">
                                      <input id="departmentsAddressStreet" class="span6 margin-right5" type="text"
                                        value="" name="departmentsAddressStreet" />
                                    </div>
                                  </li>
                                  <!-- // form item -->
                                  <li class="control-group">
                                    <label for="departmentsAddressCity" class="control-label">RUC
